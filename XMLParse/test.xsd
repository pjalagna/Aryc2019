<?schema name='employer-contacts' version='04/20/2020-1300'?>
<!-- contact info -->
<contacts id=100>
<element name="contact" id="abc">
    <complex>
        <sequence>
            <element name="Fname" maxoccurs="1" type="string" id='ccf-1/>
            <element name="Lname" id="ccl-1" maxoccurs="1" type="string"/>
            <element name="Phone" 
                     id="cce-1" minOccurs="0" maxoccurs="unbounded" type="string"/>
        </sequence>
    </complex>
</element>
<annotation id='an-c>
    <description id='ccad'> info </description>
</annotation>
</contacts>

<element name='person' ref="contact" id="mno"/>

<!-- vendor info -->
<element name='vendor' id='v'>
    <complex>
        <sequence>
            <element name="mainContact" ref="person" id='vper'/>
            <element name="secondContact" ref="person" id="vper2"/>
        </sequence>
    </complex>
</element>

<!-- supplier info -->
<suppliers>
<element name='supplier' id='Spqr'>
    <complex>
        <sequence>
            <element name="mainContact" ref="person" id='sper'/>
            <element name="secondContact" ref="person" id="sper2"/>
        </sequence>
    </complex>
</element>
<annotation id='sa'>
    <description id='sad'> info </description>
</annotation>
</suppliers>

<element name=begin id="BBB">
    <complex>
        <sequence>
            <element ref="vendor" id="bbven" minoccurs=0/>
            <element ref="supplier" id="bbsu" minoccurs=0/>
        </sequence>
    </complex>
</element>
</schema>



