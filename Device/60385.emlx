# file cvkey.py
def CVK(strin):
    """ converts any special chars into textual """
    lstrin = strin # local copy
    lstrin = lstrin.replace('!',"Bng")
    lstrin = lstrin.replace('@',"Att")
    lstrin = lstrin.replace('#',"Shp")
    lstrin = lstrin.replace('$',"Dlr")
    lstrin = lstrin.replace('%',"Pcnt")
    lstrin = lstrin.replace('^',"Ugr")
    lstrin = lstrin.replace('&',"And")
    lstrin = lstrin.replace('*',"Str")
    lstrin = lstrin.replace('(',"Oprn")
    lstrin = lstrin.replace(')',"Cprn")
    lstrin = lstrin.replace('+',"Plus")
    lstrin = lstrin.replace('=',"Eq")
    ###################################('\',"BSl") can't use char escape in 
python
    lstrin = lstrin.replace('{',"Oq")
    lstrin = lstrin.replace('}',"Cq")
    lstrin = lstrin.replace('[',"Ob")
    lstrin = lstrin.replace(']',"Cb")
    lstrin = lstrin.replace(':',"Coln")
    lstrin = lstrin.replace(';',"Semi")
    lstrin = lstrin.replace('<',"Gt")
    lstrin = lstrin.replace('>',"Lt")
    lstrin = lstrin.replace(',',"Coma")
    lstrin = lstrin.replace('.',"Prd")
    lstrin = lstrin.replace('?',"Hook")
    lstrin = lstrin.replace('/',"Slash")
    return(lstrin)
#end CVK
    
