class ini:
    """ class text """
    # interclass globals
    def __init__(self,name):
        """ sets up database for reading """
        self.dbna =3D name
        import sqlite3
        self.d =3D sqlite3.Connection(self.dbna)
    # end def init   =20
    def iniRead(self,target):
        """ reads ini table for target """
        self.stmt =3D "Select resp from TblIni where target =3D '" + =
target.__str__() + "' ;"
        self.d1 =3D self.d.execute(self.stmt)
        self.d2 =3D self.d1.fetchone()
        self.d1.close()
        return(self.d2)
    # end def iniRead
    def iniCreate(self):
        """ create table for ini """
        self.stmt =3D 'create table TblIni ( target text , resp text, =
primary key(target));'
        self.d1 =3D self.d.execute(self.stmt)
        self.d2 =3D self.d1.fetchone()
        self.d1.close()
        return(self.d2)
    # end def iniCreate
    def iniWrite(self, target,response):
        """ writes an entry into the table """
        self.stmt =3D "Insert into TblIni values ('" + target.__str__() =
+ "' , '" + response.__str__() + "' ); "
        print("test stmt =3D<<" + self.stmt + ">>")
        self.d1 =3D self.d.execute(self.stmt)
        self.d.commit()
        self.d1.close()
    #end def iniWrite
    def iniDump(self):
        self.stmt =3D 'select * from TblIni;'
        self.d1 =3D self.d.execute(self.stmt)
        ans =3D []
        self.d2 =3D self.d1.fetchone()
        while (self.d2.__str__().__len__() !=3D 4):
            ans.append(self.d2)
            self.d2 =3D self.d1.fetchone() # adj
        #wend
        return (ans)
    # end def ini dump
# end class ini


